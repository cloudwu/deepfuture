  adv.S.1.name : 计算
  adv.S.1.stage : "$(START)"
  adv.S.1.desc : "抽[card]+1;弃1[discard]"
  adv.S.1.detail : "抽 1 张卡加入手牌，然后弃掉 1 张手牌"
  adv.S.2.name : 工程
  adv.S.2.desc : "M+1"
  adv.S.2.detail : "推进进度条 M 一格，M 进度条走到顶是力量胜利的先决条件"
  adv.S.3.name : 交流
  adv.S.3.desc : "M+1"
  adv.S.3.detail : "推进进度条 M 一格，M 进度条走到顶是力量胜利的先决条件"
  adv.S.4.name : 武器
  adv.S.4.desc : "M+1"
  adv.S.4.detail : "推进进度条 M 一格，M 进度条走到顶是力量胜利的先决条件"
  adv.S.5.name : 工业
  adv.S.5.desc : "弃1[discard];抽[card]+2"
  adv.S.5.detail : "先弃掉 1 张手牌，然后抽取 2 张卡加入手牌"
  adv.S.6.name : 能量
  adv.S.6.desc : "抽[card]+1"
  adv.S.6.detail : "额外再抽 1 张卡加入手牌"
  adv.M.1.name : 艺术
  adv.M.1.desc : "C+1"
  adv.M.1.detail : "推进进度条 C 一格，C 进度条走到顶是文化胜利的先决条件"
  adv.M.2.name : 闲暇
  adv.M.2.desc : "C+2"
  adv.M.2.detail : "推进进度条 C 两格，C 进度条走到顶是文化胜利的先决条件"
  adv.M.3.name : 哲学
  adv.M.3.desc : "X+1"
  adv.M.3.detail : "推进进度条 X 一格，X 进度条走到顶是外星胜利的先决条件"
  adv.M.4.name : 文学
  adv.M.4.desc : "C+2"
  adv.M.4.detail : "推进进度条 C 两格，C 进度条走到顶是文化胜利的先决条件"
  adv.M.5.name : 政府
  adv.M.5.desc : "提升所殖民星球"
