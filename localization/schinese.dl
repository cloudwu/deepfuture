  suit.R : "[heart]"
  suit.K : "[skull]"
  suit.H : "[hand]"
  suit.F : "[foot]"
  card.corner : "${_marker}"
  card.world : "${sector} ${name}"
  card.title : "${era}. [planet]星球"
  card.adv : "${_suit} ${_name}.${era}"
  card.desc : "[blue]${_stage}[n] ${_desc}"
  START : 开始
  ADVANCE : 提升
